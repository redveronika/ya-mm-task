{
  "name": "ya-mm-task",
  "author": "Veronika Voronova",
  "scripts": {
    "start:dev": "webpack-dev-server",
    "start:prod": "NODE_ENV=production webpack -p",
    "test": "jest --no-cache"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/tests/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/app/tests/assetsTransformer.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom" : ["**/*.{js}"],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "dist/",
      "webpack/",
      "coverage/",
      "app/test",
      ".gemini.js",
      "postcss.config.js",
      "webpack.config.js"
    ]
  },
  "dependencies": {
    "babel-jest": "^20.0.3",
    "imagemin-webpack-plugin": "^1.5.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "jest-enzyme": "^4.0.1",
    "postcss-loader": "^2.0.8",
    "prop-types": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
